<!DOCTYPE html>
<html lang="{{lang}}">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>{{title}}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  {{#if csrf}}
  <meta name="csrf-token" content="{{csrf}}">
  {{/if}}
  <link rel="shortcut icon" type="image/gif" href="/Music.gif">
  <!-- Dependencies -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css"
    integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/fontawesome.css">
  <!-- Dependencies -->
  <!-- Partials -->
  <link rel="stylesheet" href="/css/partials/footer.css">
  <link rel="stylesheet" href="/css/partials/scrollbar.css">
  <link rel="stylesheet" href="/css/partials/vars.css">
  <link rel="stylesheet" href="/css/partials/sidebar.css">
  <link rel="stylesheet" href="/css/partials/navbar.css">
  <!-- Partials -->
</head>

<body>
  {{{body}}}
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8" crossorigin="anonymous">
  </script>
  {{#ifEquals filename 'server'}}
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  {{/ifEquals}}
  <link rel="stylesheet" href="/css/{{filename}}.css">
  {{#ifNotEquals filename 'main'}}
  <script src="/js/{{filename}}.js"></script>
  {{/ifNotEquals}}
  {{#if socket}}
  <script src="https://cdn.socket.io/4.0.1/socket.io.min.js"></script>
  {{/if}}
</body>

</html>